        <DrawingBrush.Drawing>
            <DrawingGroup>
                <GeometryDrawing Brush="{StaticResource brushFancyBorder}">
                    <GeometryDrawing.Geometry>
                        <LineGeometry StartPoint="5,5" EndPoint="{R}">
                    </GeometryDrawing.Geometry>
                </GeometryDrawing>
            </DrawingGroup>
        </DrawingBrush.Drawing>
    </DrawingBrush>-->
</ResourceDictionary>